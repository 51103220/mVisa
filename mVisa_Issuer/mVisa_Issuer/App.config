<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <log4net debug="true">
    <appender name="EventLogAppender" type="log4net.Appender.RollingFileAppender,log4net" >
      <file value="D:\issuerLog.txt" />
      <appendToFile value="true" />
      
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="All" />
      <appender-ref ref="EventLogAppender" />
    </root>
  </log4net>
  <appSettings>
    <add key="timeOut" value="10000"/>
    <add key="port" value="16000" />
    <add key="address" value="http://localhost" />
    <add key="mVisaEndpoint" value="https://sandbox.api.visa.com/" />
    <add key="Authorization" value="Basic N003SE1IWlZQWUNSWkZEU05WUzEyMVVKQUJtMHhjOHBUdEhDU0hQUkdNWWhBTFZsZzo0eVhOMWh1MWZmVnBHTWY1WTgwd2ZWSzUxTg==" />
    <add key="certificatePath" value="D:\mVisa\depressedQuotes_keyAndCertBundle.p12" />
    <add key="certificatePassword" value="violet1009" />
  </appSettings>
  <system.net>
    <defaultProxy enabled="true" useDefaultCredentials="false">
      <module type = "Utils.mVisaProxy, Utils" />
    </defaultProxy>
  </system.net>
</configuration>